:vpi_module "va_math";
S_010DE700 .scope module, "ALU_tb" "ALU_tb" 2 5;
 .timescale -9 -12;
v010D1D60_0 .var "i_ALUControlS", 1 0;
v010D1DB8_0 .var "i_a", 35 0;
v010DEC38_0 .var "i_b", 35 0;
v010DEC90_0 .net "o_ALU_Result", 35 0, v010DD340_0; 1 drivers
S_010D3FE8 .scope module, "dut" "ALU" 2 12, 3 2, S_010DE700;
 .timescale -9 -12;
v010DD340_0 .var "ALU_Out", 35 0;
v010D4070_0 .net "i_ALUControlS", 1 0, v010D1D60_0; 1 drivers
v010D40C8_0 .net "i_a", 35 0, v010D1DB8_0; 1 drivers
v010D1CB0_0 .net "i_b", 35 0, v010DEC38_0; 1 drivers
v010D1D08_0 .alias "o_ALU_Result", 35 0, v010DEC90_0;
E_010D7AC0 .event edge, v010D4070_0, v010D40C8_0, v010D1CB0_0, v010DD340_0;
    .scope S_010D3FE8;
T_0 ;
    %wait E_010D7AC0;
    %load/v 8, v010D4070_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.3, 6;
    %load/v 8, v010DD340_0, 36;
    %ix/load 0, 36, 0;
    %assign/v0 v010DD340_0, 0, 8;
    %jmp T_0.5;
T_0.0 ;
    %load/v 8, v010D40C8_0, 36;
    %load/v 44, v010D1CB0_0, 36;
    %add 8, 44, 36;
    %ix/load 0, 36, 0;
    %assign/v0 v010DD340_0, 0, 8;
    %jmp T_0.5;
T_0.1 ;
    %load/v 8, v010D40C8_0, 36;
    %load/v 44, v010D1CB0_0, 36;
    %sub 8, 44, 36;
    %ix/load 0, 36, 0;
    %assign/v0 v010DD340_0, 0, 8;
    %jmp T_0.5;
T_0.2 ;
    %load/v 8, v010D40C8_0, 36;
    %load/v 44, v010D1CB0_0, 36;
    %and 8, 44, 36;
    %ix/load 0, 36, 0;
    %assign/v0 v010DD340_0, 0, 8;
    %jmp T_0.5;
T_0.3 ;
    %load/v 8, v010D40C8_0, 36;
    %load/v 44, v010D1CB0_0, 36;
    %or 8, 44, 36;
    %ix/load 0, 36, 0;
    %assign/v0 v010DD340_0, 0, 8;
    %jmp T_0.5;
T_0.5 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_010DE700;
T_1 ;
    %vpi_call 2 21 "$monitor", "time=%d, i_a=%d, i_b=%d, i_ALUControlS=%2b, o_ALU_Result=%x \012", $time, v010D1DB8_0, v010DEC38_0, v010D1D60_0, v010DEC90_0;
    %movi 8, 1, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 1, 36;
    %set/v v010DEC38_0, 8, 36;
    %set/v v010D1D60_0, 0, 2;
    %delay 20000, 0;
    %movi 8, 435, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 245, 36;
    %set/v v010DEC38_0, 8, 36;
    %set/v v010D1D60_0, 0, 2;
    %delay 20000, 0;
    %movi 8, 2, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 1, 36;
    %set/v v010DEC38_0, 8, 36;
    %movi 8, 1, 2;
    %set/v v010D1D60_0, 8, 2;
    %delay 20000, 0;
    %movi 8, 5, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 6, 36;
    %set/v v010DEC38_0, 8, 36;
    %movi 8, 1, 2;
    %set/v v010D1D60_0, 8, 2;
    %delay 20000, 0;
    %movi 8, 2, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 7, 36;
    %set/v v010DEC38_0, 8, 36;
    %movi 8, 2, 2;
    %set/v v010D1D60_0, 8, 2;
    %delay 20000, 0;
    %movi 8, 8, 36;
    %set/v v010D1DB8_0, 8, 36;
    %movi 8, 11, 36;
    %set/v v010DEC38_0, 8, 36;
    %set/v v010D1D60_0, 1, 2;
    %delay 20000, 0;
    %set/v v010D1DB8_0, 1, 36;
    %movi 8, 1, 36;
    %set/v v010DEC38_0, 8, 36;
    %set/v v010D1D60_0, 0, 2;
    %delay 20000, 0;
