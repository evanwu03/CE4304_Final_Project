#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01203460 .scope module, "PC_tb" "PC_tb" 2 5;
 .timescale -9 -12;
P_0120452C .param/l "c_CLOCK_PERIOD_NS" 2 7, +C4<01100100>;
v01207BA0_0 .net "pc_out", 35 0, v01207ED8_0; 1 drivers
v01207BF8_0 .var "r_clk", 0 0;
v01207C50_0 .var "r_rst", 0 0;
S_012034E8 .scope module, "dut" "PC" 2 13, 3 12, S_01203460;
 .timescale -9 -12;
v01207A80_0 .net "i_clk", 0 0, v01207BF8_0; 1 drivers
v01207B10_0 .net "i_rst", 0 0, v01207C50_0; 1 drivers
v01207ED8_0 .var "o_pc", 35 0;
E_012045E8 .event posedge, v01207A80_0;
    .scope S_012034E8;
T_0 ;
    %wait E_012045E8;
    %load/v 8, v01207B10_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 36, 0;
    %assign/v0 v01207ED8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01207ED8_0, 36;
    %mov 44, 0, 1;
    %addi 8, 1, 37;
    %ix/load 0, 36, 0;
    %assign/v0 v01207ED8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01203460;
T_1 ;
    %set/v v01207C50_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_01203460;
T_2 ;
    %set/v v01207BF8_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_01203460;
T_3 ;
    %delay 50000, 0;
    %load/v 8, v01207BF8_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v01207BF8_0, 0, 8;
    %jmp T_3;
    .thread T_3;
    .scope S_01203460;
T_4 ;
    %ix/load 0, 1, 0;
    %assign/v0 v01207C50_0, 0, 0;
    %wait E_012045E8;
    %ix/load 0, 1, 0;
    %assign/v0 v01207C50_0, 0, 1;
    %wait E_012045E8;
    %ix/load 0, 1, 0;
    %assign/v0 v01207C50_0, 0, 0;
    %wait E_012045E8;
    %wait E_012045E8;
    %wait E_012045E8;
    %wait E_012045E8;
    %load/v 8, v01207BA0_0, 36;
    %cmpi/u 8, 3, 36;
    %jmp/0xz  T_4.0, 4;
    %vpi_call 2 41 "$display", "Test Passed - Correct PC value";
    %jmp T_4.1;
T_4.0 ;
    %vpi_call 2 44 "$display", "Test Failed - Incorrect PC value";
T_4.1 ;
    %vpi_call 2 46 "$finish";
    %end;
    .thread T_4;
    .scope S_01203460;
T_5 ;
    %vpi_call 2 52 "$dumpfile", "dump.vcd";
    %vpi_call 2 53 "$dumpvars", 1'sb0;
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
